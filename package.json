{
  "name": "gentzen-seqcalc",
  "version": "1.0.0",
  "description": "An automated propositional logic prover",
  "homepage": "https://github.com/sciencesakura/gentzen-seqcalc",
  "scripts": {
    "clean:rmdir": "rimraf ./docs",
    "clean:mkdir": "mkdirp ./docs",
    "clean": "run-s clean:rmdir clean:mkdir",
    "format:html": "prettier --write src/**/*.html",
    "format:ts": "prettier --write src/ts/**/*.ts",
    "format": "run-p format:*",
    "lint:html": "htmlhint src/**/*.html",
    "lint:scss": "stylelint --fix src/scss/**/*.scss",
    "lint:ts": "eslint --fix src/ts/**/*.ts",
    "lint": "run-p lint:*",
    "test:ts": "jest",
    "build:html": "html-minifier -o docs/index.html -c .html-minifier.json src/index.html",
    "build:scss": "node-sass -r -o docs/css --include-path ./node_modules --output-style compressed src/scss",
    "build:ts": "webpack --mode production",
    "build": "run-p build:*",
    "build-all": "run-s clean format lint build"
  },
  "author": "sciencesakura",
  "repository": {
    "type": "git",
    "url": "https://github.com/sciencesakura/gentzen-seqcalc.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "html-minifier": "^4.0.0",
    "htmlhint": "^0.11.0",
    "jest": "^25.1.0",
    "mkdirp": "^1.0.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "stylelint": "^13.2.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "bulma": "^0.8.0"
  }
}
