{
  "name": "gentzen-seqcalc",
  "version": "1.0.0",
  "description": "An automated propositional logic prover",
  "homepage": "https://github.com/sciencesakura/gentzen-seqcalc",
  "private": true,
  "scripts": {
    "clean": "rm -rf docs/",
    "format:html": "prettier --write src/**/*.html",
    "format:scss": "prettier --write src/scss/**/*.scss",
    "format:ts": "prettier --write src/ts/**/*.ts",
    "format": "run-p format:*",
    "lint:html": "htmlhint src/**/*.html",
    "lint:scss": "stylelint --fix src/scss/**/*.scss",
    "lint:ts": "eslint --fix src/ts/**/*.ts",
    "lint": "run-p lint:*",
    "test:ts": "jest",
    "build:html": "html-minifier -o docs/index.html -c .html-minifier.json src/index.html",
    "build:scss": "sass -I node_modules src/scss/index.scss:docs/css/index.css",
    "build:ts": "webpack --mode production",
    "build": "run-p build:*",
    "build-all": "run-s format lint build"
  },
  "author": "sciencesakura",
  "repository": {
    "type": "git",
    "url": "https://github.com/sciencesakura/gentzen-seqcalc.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "html-minifier": "^4.0.0",
    "htmlhint": "^0.11.0",
    "jest": "^25.5.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "sass": "^1.26.5",
    "stylelint": "^13.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.2",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "bulma": "^0.8.0"
  }
}
